{
  "name": "brainsum_drupal8-9_startertheme",
  "version": "2.4.1",
  "description": "Drupal 8-9 coding standards ready startertheme powered by Laravel Mix (Webpack) without any dependency (base theme).",
  "repository": {
    "type": "git",
    "url": "https://github.com/brainsum/drupal8-startertheme.git"
  },
  "author": "Krisztian Pinter <kpinter@brainsum.com>",
  "private": true,
  "keywords": [
    "Drupal",
    "Drupal 8",
    "Drupal 9",
    "Laravel Mix",
    "php",
    "Sass",
    "theme",
    "Twig",
    "Webpack"
  ],
  "license": "ISC",
  "scripts": {
    "dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run dev -- --watch",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "start": "npm run watch"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.10",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "browser-sync": "^2.26.13",
    "browser-sync-webpack-plugin": "^2.2.2",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.9",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-vue": "^7.2.0",
    "laravel-mix": "^5.0.9",
    "laravel-mix-eslint": "^0.1.3",
    "laravel-mix-glob": "^1.1.4",
    "postcss-custom-properties": "^10.0.0",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^2.1.1"
  },
  "resolutions": {
    "yargs-parser": "^13.1.2"
  },
  "dependencies": {
    "custom-event-polyfill": "^1.0.7"
  }
}
